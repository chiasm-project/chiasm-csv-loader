{
  "name": "chiasm-dataset-loader",
  "version": "0.3.1",
  "description": "A Chiasm plugin that loads datasets over HTTP.",
  "main": "index.js",
  "scripts": {
    "pretest": "browserify -g browserify-shim -o chiasm-dataset-loader.js -s ChiasmDatasetLoader index.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chiasm-project/chiasm-dataset-loader.git"
  },
  "keywords": [
    "data",
    "dsv",
    "csv",
    "tsv",
    "d3"
  ],
  "author": "Curran Kelleher",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chiasm-project/chiasm-dataset-loader/issues"
  },
  "homepage": "https://github.com/chiasm-project/chiasm-dataset-loader#readme",
  "browserify-shim": {
    "d3": "global:d3",
    "model-js": "global:Model",
    "chiasm-component": "global:ChiasmComponent"
  },
  "dependencies": {
    "chiasm-component": "^0.2.2",
    "d3": "^3.5.6",
    "dsv-dataset": "^0.2.0",
    "model-js": "^0.2.4"
  },
  "devDependencies": {
    "browserify": "^11.2.0",
    "browserify-shim": "^3.8.10",
    "chai": "^3.3.0",
    "chiasm": "^0.2.2",
    "mocha": "^2.3.3",
    "xhr2": "^0.1.3"
  }
}
